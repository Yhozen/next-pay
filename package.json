{
  "name": "nextpay-monorepo",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"*/.{ts,tsx,md}\"",
    "oss": "npx ts-node opensource.ts"
  },
  "devDependencies": {
    "eslint-config-drimo": "*",
    "prettier": "latest",
    "turbo": "^1.10.13",
    "typescript": "5.0"
  },
  "engines": {
    "node": "18"
  },
  "dependencies": {},
  "packageManager": "yarn@1.22.19",
  "volta": {
    "node": "18.12.0",
    "yarn": "1.22.19"
  }
}
